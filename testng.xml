<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Suite (Re-running failing test using IRetryAnalyzer)">
	<listeners>
		<listener class-name="listeners.ReportListeners" />
		<listener class-name="listeners.RetryTransformer" />
	</listeners>
	<test name="Regression Test">
		<classes>
			<class name="tests.LoginTest" />
			<class name="tests.LogoutTest" />
		</classes>
	</test> <!-- Regression Test -->
</suite> <!-- Automation Suite -->
